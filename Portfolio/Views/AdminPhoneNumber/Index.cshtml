@using Entities
@model List<AdminPhoneNumber>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<table class="table table-hover text-center">
    <thead>
        <tr>
            <th>ID</th>
            <th>Telefon Numarası</th>
            <th><a href="/AdminPhoneNumber/Add" type="button" class="btn btn-success">Telefon Numarası Ekle</a></th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.PhoneNumber</td>
                <td><a type="button" class="btn btn-danger " style="margin-right:5px;" href="/AdminPhoneNumber/AdminDeletePhoneNumber/@item.Id">SİL</a><a type="button" class="btn btn-warning" href="/AdminPhoneNumber/AdminUpdatePhoneNumber/@item.Id">Güncelle</a></td>

            </tr>
        }
    </tbody>
</table>

